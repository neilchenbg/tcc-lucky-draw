<?xml version='1.0' encoding='utf-8'?>
<widget id="com.campuscruiser.tccluckydraw" ios-CFBundleVersion="1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TccLuckyDraw</name>
    <description>
        TccLuckyDraw APP
    </description>
    <author email="ccsupport@campuscruiser.com" href="http://www.campuscruiser.com/">
        Campuscruiser APP team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="29" platform="ios" src="www/images/icons/ios/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="www/images/icons/ios/icon-small-2x.png" width="58" />
        <icon height="40" platform="ios" src="www/images/icons/ios/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="www/images/icons/ios/icon-40-2x.png" width="80" />
        <icon height="50" platform="ios" src="www/images/icons/ios/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="www/images/icons/ios/icon-50-2x.png" width="100" />
        <icon height="57" platform="ios" src="www/images/icons/ios/icon.png" width="57" />
        <icon height="114" platform="ios" src="www/images/icons/ios/icon-2x.png" width="114" />
        <icon height="60" platform="ios" src="www/images/icons/ios/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="www/images/icons/ios/icon-60-2x.png" width="120" />
        <icon height="180" platform="ios" src="www/images/icons/ios/icon-60-3x.png" width="180" />
        <icon height="72" platform="ios" src="www/images/icons/ios/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="www/images/icons/ios/icon-72-2x.png" width="144" />
        <icon height="76" platform="ios" src="www/images/icons/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="www/images/icons/ios/icon-76-2x.png" width="152" />
        <splash height="1024" platform="ios" src="www/images/screens/ios/splash-768x1024.png" width="768" />
        <splash height="768" platform="ios" src="www/images/screens/ios/splash-1024x768.png" width="1024" />
        <splash height="2048" platform="ios" src="www/images/screens/ios/splash-768x1024@2x.png" width="1536" />
        <splash height="1536" platform="ios" src="www/images/screens/ios/splash-1024x768@2x.png" width="2048" />
    </platform>
    <engine name="ios" spec="~4.3.1" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.1" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.2" />
</widget>
